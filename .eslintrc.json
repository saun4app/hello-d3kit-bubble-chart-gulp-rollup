{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "ENV": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "linebreak-style": 0,
    "no-unused-vars": [0, { "args": "none" }],
    "no-undef": 0,
    "no-console": 0
  }
}
